{
  "name": "typeflow-nodes-example",
  "version": "0.1.0",
  "description": "Example custom nodes for Typeflow",
  "license": "MIT",
  "homepage": "https://github.com/your-org/typeflow-nodes-example",
  "author": {
    "name": "Your Name",
    "email": "your@email.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/typeflow-nodes-example.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "copyfiles -u 1 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "typeflow": {
    "nodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExampleApi.credentials.js",
      "dist/credentials/Postgres.credentials.js",
      "dist/credentials/MongoDb.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HttpRequest/HttpRequest.node.js",
      "dist/nodes/DataTransform/DataTransform.node.js",
      "dist/nodes/Postgres/Postgres.node.js",
      "dist/nodes/MongoDb/MongoDb.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "typeflow-community-node-package"
  ]
}
